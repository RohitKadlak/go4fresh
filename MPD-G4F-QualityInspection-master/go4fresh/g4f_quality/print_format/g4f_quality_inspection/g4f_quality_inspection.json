 "doctype": "Print Format",
 "font": "Default",
 "html": "<!DOCTYPE html>\n<html>\n    <head>\n        \n    </head>\n    <body>\n        <div class=\"row\">\n            <div class=\"col-xs-2\">\n                <div class=\"letter-head\" style=\"height:125px; width:125px;text-align:center;\">{{letter_head}}</div>\n            </div>\n            <div class=\"col-xs-10\">\n                <div id=\"head\" style=\" font-family: Tenorite Bold; font-size: 24px; text-align: center;font-weight: bold;\">\n                    FRESH PRODUCE VALUE CREATION SERVICES PRIVATE LIMITED\n                </div>\n                <div class=\"col-xs-9\" id=\"head\" style=\" font-family: Tenorite Bold; font-size: 12px;font-weight: bold; text-align: center;\">\n                    7, Office No-706, Cyber One IT Park,Sector 30 A, Vashi,<br>\n                    Navi Mumbai,Maharashtra, State Code: 27<br>\n                    India<br>\n                    Postal Code: 400703<br>\n                    Phone: 022-48972571 <br>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\" >\n                <div id=\"head\" style=\" font-family: Tenorite Bold; font-size: 18px; text-align: center;font-weight: bold;\">\n                    Quality Inspection Report&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <div>\n                    <table width=\"100%\" style=\"border: 1px solid balck;font-size: 11px;\">\n                        <tr>\n                            <td style=\"border: 1px solid black;\"><b>Consignee :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.consignee_name}}</td>\n                            <td style=\"border: 1px solid black;\"><b>Origin :</b></td>\n                            <td style=\"border: 1px solid black;\">India</td>\n                            <td style=\"border: 1px solid black;\"><b>Qty :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.grn_quantity}}</td>\n                            <td style=\"border: 1px solid black;\"><b>Received Date :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.receipt_date}}</td>\n                        </tr>\n                        <tr>\n                            <td style=\"border: 1px solid black;\"><b>Commodity :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.product_name}}</td>\n                            <td style=\"border: 1px solid black;\"><b>Container :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.container_no}}</td>\n                            <td style=\"border: 1px solid black;\"><b>Pulp Temp :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.pulp_temp}}</td>\n                            <td style=\"border: 1px solid black;\"><b>Inspection Date :</b></td>\n                            <td style=\"border: 1px solid black;\">{{doc.get_formatted(\"inspection_date\")}}</td>\n                        </tr>\n                    </table>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n              <div>\n                <table width=\"100%\" style=\"margin-top:2px;border: 1px solid balck;font-size: 11px;\">\n                  <tr>\n                    <th style=\"border: 1px solid black;text-align: center;\">Variety/Goods Description</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Package</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Bags Sample</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Grams</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Gross Wt.Kg.</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Net Wt.Kg.</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Size (MM)</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Small</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Without Patti</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Black Smugs</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Visual Double</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Injury</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Rotten</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">% Sprouting</th>\n                    <th style=\"border: 1px solid black;text-align: center;\">Total Defects (In%)</th>\n                  </tr>\n                  {% for item in doc.qi_sample %}\n                    <tr>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.product_name}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.package_size_for_sampling}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">1</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{(item.gross_weight*1000)|round|int}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.gross_weight}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.net_weightkg}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.size}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"small_size_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"without_patti_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"black_smugs_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"dry_roots_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"injury_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"rotten_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"sprouting_in_percentage\")}}</td>\n                      <td style=\"border: 1px solid black;text-align: center;\">{{item.get_formatted(\"total_defects_percentage\")}}</td>\n                    </tr>\n                  {% endfor %}\n    \n                  {% if doc.total_gross_weight %}\n                    {% set vars = {'bags_sample':doc.qi_sample|length,'gross_weight':0.00,'net_weight':0.00, 'small_size':0.00,'without_patti':0.00,'black_smugs':0.00,'dry_roots':0.00,'injury': 0.00, 'rotten':0.00, 'sprouting':0.00,'total_defects':0.00} %}\n                    {% for i in doc.qi_sample %}\n                      {% if vars.update(\n                        {\n                            'gross_weight': vars.gross_weight + i.gross_weight,\n                            'net_weight': vars.net_weight + i.net_weightkg,\n                            'small_size': vars.small_size + i.small_size_in_percentage,\n                            'without_patti': vars.without_patti + i.without_patti_in_percentage,\n                            'black_smugs': vars.black_smugs + i.black_smugs_in_percentage,\n                            'dry_roots': vars.dry_roots + i.dry_roots_in_percentage,\n                            'injury': vars.injury + i.injury_in_percentage,\n                            'rotten': vars.rotten + i.rotten_in_percentage,\n                            'sprouting': vars.sprouting + i.sprouting_in_percentage,\n                            'total_defects':vars.total_defects + i.total_defects_percentage\n                        }\n                      )%}{% endif %}\n                    {% endfor %}\n                    {% set gross_weight = vars.gross_weight/vars.bags_sample %}\n                    {% set net_weight = vars.net_weight/vars.bags_sample %}\n                    {% set small_size = vars.small_size/vars.bags_sample %}\n                    {% set without_patti = vars.without_patti/vars.bags_sample %}\n                    {% set black_smugs = vars.black_smugs/vars.bags_sample %}\n                    {% set dry_roots = vars.dry_roots/vars.bags_sample %}\n                    {% set injury =  vars.injury/vars.bags_sample %}\n                    {% set sprouting = vars.sprouting/vars.bags_sample %}\n                    {% set rotten = vars.rotten/vars.bags_sample %}\n                    {% set total_defects = vars.total_defects/vars.bags_sample %}\n                    <tr>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>Average</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b></b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{vars.bags_sample}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b></b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(gross_weight|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(net_weight|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b></b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(small_size|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(without_patti|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(black_smugs|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(dry_roots|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(injury|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(rotten|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(sprouting|float)}}</b></td>\n                      <td style=\"border: 1px solid black;text-align: center;\"><b>{{'%0.2f'| format(total_defects|float)}}</b></td>\n                    </tr>\n                  {% endif %}\n    \n                </table>\n              </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n              <div>\n                <table width=\"100%\">\n                    <tr>\n                        <td width=\"40%\">\n                            <table width=\"100%\" style=\"border: 1px solid balck;font-size: 11px;\">\n                                <tr>\n                                    <th style=\"border: 1px solid black;text-align: center;\">\n                                        Inspection Results\n                                    </th>\n                                </tr>\n                                <tr>\n                                    <td style=\"border: 1px solid black;\">\n                                        {{doc.remarks}}\n                                    </td>\n                                </tr>\n                            </table>\n                        </td>\n                        <td width=\"20%\">Prepared By:</td>\n                        <td width=\"20%\">Verified By:</td>\n                        <td width=\"20%\">Confirmed By:</td>\n                    </tr>\n                </table>\n              </div>\n            </div>\n        </div>\n    </body>\n</html>",
